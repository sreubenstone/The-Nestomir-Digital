{
  "expo": {
    "name": "The Nestomir",
    "slug": "theNestomir",
    "privacy": "unlisted",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/ios_icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "notification": {
      "icon": "./assets/images/ios_icon.png",
      "color": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thenestomir.thenestomir",
      "buildNumber": "1.0.6"
    },
    "android": {
      "package": "com.thenestomir.thenestomirapp",
      "googleServicesFile": "./google-services.json",
      "icon": "./assets/images/android_base_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreground_android_icon.png"
      },
      "permissions": [],
      "versionCode": 2
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "the_nestomir",
            "project": "app",
            "authToken": ""
          }
        }
      ]
    }
  }
}
